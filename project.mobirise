        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".category-wrap > * when (@showCardsCategory), (@showCardsDate), (@showCardsInfo)": {
              "display": "inline-block",
              "padding-right": "14px"
            },
            ".category-wrap when (@showLinePost)": {
              "& when (@showCardsCategory), (@showCardsDate)": {
                ".card-category, .card-date": {
                  "&:before": {
                    "content": "''",
                    "display": "inline-block",
                    "background-color": "@warningColor"
                  },
                  "& when (@showEffectLinePost)": {
                    "&:before": {
                      "-webkit-transition": "all .2s ease",
                      "transition": "all .2s ease"
                    },
                    "&:hover:before": {
                      "transform": "rotate(90deg)",
                      "background-color": "@primaryColor"
                    }
                  }
                },
                ".card-category:before when (@showCardsInfo)": {
                  "height": "12px",
                  "width": "1px",
                  "margin-right": "14px"
                },
                ".card-date:before when (@showCardsCategory), (@showCardsInfo)": {
                  "height": "12px",
                  "width": "1px",
                  "margin-right": "14px"
                }
              }
            },
            "@media (min-width: 768px)": {
              "& when (@showLineText)": {
                "& when (@showCardsTitle), (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
                  ".card-text-box, .category-wrap": {
                    "padding-left": "50px"
                  },
                  ".card-text-box": {
                    "position": "relative",
                    "&:before": {
                      "content": "\"\"",
                      "position": "absolute",
                      "top": "14px",
                      "left": "0",
                      "width": "30px",
                      "height": "3px",
                      "background-color": "@primaryColor"
                    }
                  }
                }
              }
            },
            ".title when (@showMilestone) and not (@titleAlign = '2')": {
              ".milestone > div > div": {
                "padding-left": "0",
                "padding-right": "0"
              },
              ".milestone, .milestone > div, .milestone > div > div": {
                "width": "100%"
              },
              ".milestone > div > div when (@showBorderBottom)": {
                "border-bottom": "1px solid @milBorderColor"
              },
              ".milestone .mbr-text": {
                "padding-top": "5px"
              },
              ".milestone > div when (@showTitle), (@showSubtitle)": {
                "& when (@milCount > 2)": {
                  "&:nth-child(1), &:nth-child(2)": {
                    "padding-bottom": "20px"
                  }
                }
              },
              "@media (max-width: 767px)": {
                ".title-wrap": {
                  "padding-bottom": "20px"
                },
                ".milestone > div:not(:last-child)": {
                  "padding-bottom": "20px"
                },
                ".milestone .numeral": {
                  "justify-content": "center"
                }
              }
            },
            ".card-box, .card-box > *:not(.mbr-overlay)": {
              "position": "relative"
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-img when (@showImgOverlay) and (@showImg)": {
              "amp-img": {
                "position": "relative",
                "&:before": {
                  "content": "\"\"",
                  "position": "absolute",
                  "height": "100%",
                  "width": "100%",
                  "z-index": "1",
                  "pointer-events": "none",
                  "-webkit-transition": "all 0.2s ease",
                  "transition": "all 0.2s ease"
                },
                "> a:before when (@showImgButton)": {
                  "content": "\"＋\"",
                  "position": "absolute",
                  "top": "50%",
                  "left": "50%",
                  "font-size": "40px",
                  "transform": "translate(-50%, -50%)",
                  "color": "transparent",
                  "z-index": "2",
                  "-webkit-transition": "all 0.2s ease",
                  "transition": "all 0.2s ease"
                }
              },
              "&:hover": {
                "amp-img:before": {
                  "background-color": "@imgOverlayColor",
                  "opacity": "@imgOverlayOpacity"
                },
                "amp-img > a:before when (@showImgButton)": {
                  "color": "contrast(@imgOverlayColor, darken(@imgOverlayColor, 50%), #ffffff, 60%)"
                }
              }
            }
          },
          "_name": "features2",
