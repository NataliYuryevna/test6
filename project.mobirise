              "&:hover:after": {
                "opacity": "@imgOverlayOpacity",
                "background": "@imgOverlayColor"
              },
              "amp-img > a:before when (@showImgButton)": {
                "content": "\"＋\"",
                "position": "absolute",
                "top": "50%",
                "left": "50%",
                "font-size": "40px",
                "transform": "translate(-50%, -50%)",
                "z-index": "1",
                "color": "transparent",
                "-webkit-transition": "color 0.2s",
                "transition": "color 0.2s"
              },
              "&:hover amp-img > a:before when (@showImgButton)": {
                "color": "contrast(@imgOverlayColor, darken(@imgOverlayColor, 50%), #ffffff, 60%)"
              }
            }
          },
          "_name": "slider2",
          "_customHTML": "<section class=\"slider2 slider\" group=\"Teams\" plugins=\"amp-main, amp-carousel\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Width Images\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone\">    \n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone\">\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>    \n    <header>Images</header>\n        <input type=\"range\" inline=\"\" title=\"Slider Height\" name=\"slideHeight\" min=\"300\" max=\"700\" step=\"10\" value=\"390\">\n        <input type=\"range\" inline=\"\" title=\"Count Images Slider\" name=\"countImg\" min=\"1\" max=\"4\" step=\"1\" value=\"3\">\n        <select title=\"Sliders Count\" name=\"slidersAmount\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected=\"\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n        </select>\n        <input type=\"color\" title=\"Buttons Color\" name=\"buttonsColor\" value=\"#273272\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showImgTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showImgText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line Before Text\" name=\"lineText\" condition=\"showImgText\" checked=\"\">\n        <select title=\"Align\" name=\"textImgAlign\" condition=\"showImgTitle || showImgText\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Image Overlay\" name=\"showImgOverlay\" checked=\"\">\n        <input type=\"color\" title=\"Image Overlay Color\" name=\"imgOverlayColor\" value=\"#000000\" condition=\"showImgOverlay\">\n        <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imgOverlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"showImgOverlay\">\n        <input type=\"checkbox\" title=\"Show Image Button\" name=\"showImgButton\" condition=\"showImgOverlay\" checked=\"\">\n    <header>Text</header>    \n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showCardSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showCardText\">\n        <input type=\"checkbox\" title=\"List\" name=\"showCardList\" checked=\"\">\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"listCount\" min=\"1\" max=\"9\" step=\"1\" value=\"1\" condition=\"showCardList\"> \n            <select title=\"Stylized Counters\" name=\"stylizedCounters\" condition=\"showCardList\">\n                <option value=\"1\">Counters</option>\n                <option value=\"2\" selected=\"\">Markers</option>\n                <option value=\"3\">Icons</option>\n            </select>\n            <input type=\"checkbox\" title=\"Border Bottom Counters\" name=\"countersBorder\" checked=\"\" condition=\"showCardList &amp;&amp; stylizedCounters == 1\">\n            <input type=\"range\" inline=\"\" title=\"Width Marker\" name=\"widthImg\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"0.8\" condition=\"showCardList\">\n            <input type=\"checkbox\" title=\"Title List\" name=\"showCardListTitle\" condition=\"showCardList\">\n            <input type=\"checkbox\" title=\"Text List\" name=\"showCardListText\" checked=\"\" condition=\"showCardList\">\n        <input type=\"checkbox\" title=\"Milestone With Number\" name=\"showMilestone\" checked=\"\">\n            <input type=\"checkbox\" title=\"Border Top\" name=\"showBorderTop\" condition=\"showMilestone\" checked=\"\">\n            <input type=\"checkbox\" title=\"Border Right\" name=\"showBorderRight\" condition=\"showMilestone &amp;&amp; milCount != 1 &amp;&amp; columnWidth != 5\" checked=\"\">\n            <input type=\"color\" title=\"Color Border\" name=\"borderColor\" value=\"#bec1c7\" condition=\"showMilestone &amp;&amp; (showBorderTop || (showBorderRight &amp;&amp; milCount != 1))\">\n            <input type=\"range\" inline=\"\" title=\"Count Milestone\" name=\"milCount\" min=\"1\" max=\"4\" step=\"1\" value=\"2\" condition=\"showMilestone\">\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Padding\" name=\"showPadding\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone\" checked=\"\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardTitle || showCardSubtitle || showCardText || showCardList || showMilestone\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#bec1c7\" condition=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardText || showCardList || showMilestone)\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardText || showCardList || showMilestone)\">\n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background2.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#ffffff\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title-wrap mbr-pb-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left': titleAlign==1,\n                         'align-center': titleAlign==2,\n                         'align-right': titleAlign==3, 'mbr-px-2': fullWidth}\">\n            <h1 class=\"mbr-section-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\">Slider Team</h1>\n            <h2 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".title-wrap .mbr-section-subtitle\">Lorem ipsum dolor sit amet, consectetur adipisicing elit.</h2>\n        </div> \n        <div class=\"mbr-row\" mbr-class=\"{'mbr-px-2': !transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone)}\">\n            <div class=\"elements-block mbr-flex flex-wrap mbr-jc-c\" mbr-class=\"{'mbr-row-reverse':reverseContent &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone), 'mbr-p-4': !transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone)}\">\n                <div class=\"mbr-overlay\" mbr-if=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone)\"></div>\n                <div class=\"second-element mbr-flex mbr-col-sm-12 mbr-col-md-12\" mbr-if=\"showCardTitle || showCardSubtitle || showCardText || showMilestone || showCardList\" mbr-class=\"{'mbr-col-lg-8':columnWidth==1,\n                                 'mbr-col-lg-7':columnWidth==2,\n                                 'mbr-col-lg-6':columnWidth==3,\n                                 'mbr-col-lg-5':columnWidth==4,\n                                 'mbr-col-lg-4':columnWidth==5}\">\n                    <div class=\"second-element-box mbr-m-auto\" mbr-class=\"{'align-center': cardAlign==2,\n                                 'align-right': cardAlign==3, 'mbr-px-2': transparentBg &amp;&amp; fullWidth}\">\n                        <h2 class=\"element-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showCardTitle\" data-app-selector=\".element-title\">Team</h2>\n                        <h3 class=\"element-subtitle mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showCardSubtitle\" data-app-selector=\".element-subtitle\">of Company</h3>\n                        <div class=\"mbr-list\" mbr-list=\"\" mbr-list-grow=\"listCount\" mbr-if=\"showCardList\" mbr-class=\"{'mbr-pt-4': showCardTitle || showCardSubtitle}\">\n                            <div class=\"mbr-flex items-list\" mbr-class=\"{'mbr-jc-c':cardAlign==2, 'mbr-jc-e': cardAlign==3}\">\n                                <div class=\"marker-wrap\" mbr-if=\"stylizedCounters == 1 || stylizedCounters == 2\"></div>\n                                <div class=\"icon-wrap\" mbr-if=\"stylizedCounters == 3\">\n                                    <span mbr-icon=\"\" class=\"amp-iconfont fa-check fa\"></span>\n                                </div>\n                                <div class=\"list-box\" mbr-if=\"showCardListTitle || showCardListText\">\n                                    <h4 class=\"list-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" mbr-if=\"showCardListTitle\" data-app-selector=\".mbr-list .list-title\" mbr-class=\"{'mbr-pb-1':showCardListText}\">\n                                        First Title\n                                    </h4>\n                                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardListText\" data-app-selector=\".mbr-list .mbr-text\">\n                                       Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</p>\n                                 </div>\n                            </div> \n                        </div>\n                        <p class=\"mbr-text element-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardText\" data-app-selector=\".element-text\" mbr-class=\"{'mbr-pt-3':showCardTitle || showCardSubtitle || showCardList}\">Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.</p>\n                        <div class=\"milestone mbr-row mbr-px-2\" mbr-if=\"showMilestone\" mbr-list=\"\" mbr-list-grow=\"milCount\" mbr-class=\"{'mbr-pt-4':!showBorderTop &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText), 'align-center': columnWidth == 5}\">\n                            <div class=\"mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-lg-6 mbr-col-xl-4':milCount == 3 &amp;&amp; fullWidth &amp;&amp; columnWidth != 5, 'mbr-col-md-6 mbr-col-lg-6': milCount > 1  &amp;&amp; columnWidth != 5, 'mbr-col-xl-6': milCount > 1 &amp;&amp; milCount != 3 &amp;&amp; fullWidth &amp;&amp; columnWidth != 5, 'mbr-col-md-12 mbr-col-lg-12': milCount == 1 || columnWidth == 5}\">\n                                <div class=\"numeral mbr-flex\" mbr-class=\"{'mbr-jc-c': cardAlign == 2 || columnWidth == 5, 'mbr-jc-e': cardAlign == 3 &amp;&amp; columnWidth != 5}\">\n                                    <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".milestone .numeral\">\n                                        100\n                                    </h4>\n                                    <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" data-app-selector=\".milestone .numeral\">\n                                        %\n                                    </h4>\n                                </div>\n                                <p class=\"mbr-text mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" data-app-selector=\".milestone .mbr-text\">\n                                   Lorem ipsum</p>\n                            </div>\n                            \n                        <div class=\"mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-lg-6 mbr-col-xl-4':milCount == 3 &amp;&amp; fullWidth &amp;&amp; columnWidth != 5, 'mbr-col-md-6 mbr-col-lg-6': milCount > 1  &amp;&amp; columnWidth != 5, 'mbr-col-xl-6': milCount > 1 &amp;&amp; milCount != 3 &amp;&amp; fullWidth &amp;&amp; columnWidth != 5, 'mbr-col-md-12 mbr-col-lg-12': milCount == 1 || columnWidth == 5}\">\n                                <div class=\"numeral mbr-flex\" mbr-class=\"{'mbr-jc-c': cardAlign == 2 || columnWidth == 5, 'mbr-jc-e': cardAlign == 3 &amp;&amp; columnWidth != 5}\">\n                                    <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".milestone .numeral\">\n                                        100\n                                    </h4>\n                                    <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" data-app-selector=\".milestone .numeral\">\n                                        %\n                                    </h4>\n                                </div>\n                                <p class=\"mbr-text mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" data-app-selector=\".milestone .mbr-text\">\n                                   Lorem ipsum</p>\n                            </div></div>\n                    </div>\n                </div>\n                <div class=\"image-wrap mbr-col-md-12 mbr-col-sm-12\" mbr-class=\"{'mbr-col-lg-8':columnWidth==5,\n                                 'mbr-col-lg-7':columnWidth==4,\n                                 'mbr-col-lg-6':columnWidth==3,\n                                 'mbr-col-lg-5':columnWidth==2,\n                                 'mbr-col-lg-4':columnWidth==1,\n                                 'mbr-col-lg-12': !showCardTitle &amp;&amp; !showCardSubtitle &amp;&amp; !showCardText &amp;&amp; !showMilestone &amp;&amp; !showCardList}\">\n                    <div class=\"slider-box\">\n                        <amp-carousel layout=\"fixed-height\" type=\"carousel\" loop=\"\" height=\"{{slideHeight}}\">\n                            <div class=\"carousel-img mbr-flex mbr-column\" mbr-class=\"{'mbr-col-md-6 mbr-col-xl-6':countImg == 2, 'mbr-col-md-4':countImg == 3, 'mbr-col-md-6 mbr-col-lg-3':countImg == 4, 'mbr-col-md-12':countImg == 1}\">\n                                <div class=\"gallery-img-wrap mobirise-loader\" mbr-class=\"{'img-with-overlay': showImgOverlay}\">\n                                    <amp-img src=\"@PROJECT_PATH@/assets/images/face3.jpg\" height=\"390\" alt=\"Mobirise\" layout=\"responsive\" width=\"390\">\n                                        <div placeholder=\"\" class=\"placeholder\"></div>\n                                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/face3.jpg\" alt=\"Mobirise\" title=\"\">\n                                    </amp-img>\n                                </div>\n                                <div class=\"text-wrap\" mbr-if=\"showImgTitle || showImgText\" mbr-class=\"{'mbr-px-2': !transparentBg &amp;&amp; countImg == 1, 'mbr-col-sm-12':(!transparentBg &amp;&amp; countImg != 1) || (transparentBg &amp;&amp; countImg == 1)}\">\n                                    <div class=\"text-content\" mbr-class=\"{'align-center': textImgAlign == 2,'align-right': textImgAlign == 3}\">\n                                        <h3 class=\"mbr-content-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-content-title\" mbr-if=\"showImgTitle\" mbr-class=\"{'mbr-pb-1': showImgText}\">Olivia Snow</h3>\n                                        <h4 class=\"mbr-content-subtitle mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" data-app-selector=\".text-content .mbr-content-subtitle\" mbr-if=\"showImgText\">Lorem ipsum</h4>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"carousel-img mbr-flex mbr-column\" mbr-class=\"{'mbr-col-md-6 mbr-col-xl-6':countImg == 2, 'mbr-col-md-4':countImg == 3, 'mbr-col-md-6 mbr-col-lg-3':countImg == 4, 'mbr-col-md-12':countImg == 1, 'super-hide': slidersAmount < 2}\" mbr-if=\"slidersAmount > 1\">\n                                <div class=\"gallery-img-wrap mobirise-loader\" mbr-class=\"{'img-with-overlay': showImgOverlay}\">\n                                    <amp-img src=\"@PROJECT_PATH@/assets/images/face2.jpg\" height=\"390\" alt=\"Mobirise\" layout=\"responsive\" width=\"390\">\n                                        <div placeholder=\"\" class=\"placeholder\"></div>\n                                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/face2.jpg\" alt=\"Mobirise\" title=\"\">\n                                    </amp-img>\n                                </div>\n                                <div class=\"text-wrap\" mbr-if=\"showImgTitle || showImgText\" mbr-class=\"{'mbr-px-2': !transparentBg &amp;&amp; countImg == 1, 'mbr-col-sm-12':(!transparentBg &amp;&amp; countImg != 1) || (transparentBg &amp;&amp; countImg == 1)}\">\n                                    <div class=\"text-content\" mbr-class=\"{'align-center': textImgAlign == 2,'align-right': textImgAlign == 3}\">\n                                        <h3 class=\"mbr-content-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-content-title\" mbr-if=\"showImgTitle\" mbr-class=\"{'mbr-pb-1': showImgText}\">Rafe Stevenson</h3>\n                                        <h4 class=\"mbr-content-subtitle mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" data-app-selector=\".text-content .mbr-content-subtitle\" mbr-if=\"showImgText\">Lorem ipsum</h4>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"carousel-img mbr-flex mbr-column\" mbr-class=\"{'mbr-col-md-6 mbr-col-xl-6':countImg == 2, 'mbr-col-md-4':countImg == 3, 'mbr-col-md-6 mbr-col-lg-3':countImg == 4, 'mbr-col-md-12':countImg == 1, 'super-hide': slidersAmount < 3}\" mbr-if=\"slidersAmount > 2\">\n                                <div class=\"gallery-img-wrap mobirise-loader\" mbr-class=\"{'img-with-overlay': showImgOverlay}\">\n                                    <amp-img src=\"@PROJECT_PATH@/assets/images/face4.jpg\" height=\"390\" alt=\"Mobirise\" layout=\"responsive\" width=\"390\">\n                                        <div placeholder=\"\" class=\"placeholder\"></div>\n                                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/face4.jpg\" alt=\"Mobirise\" title=\"\">\n                                    </amp-img>\n                                </div>\n                                <div class=\"text-wrap\" mbr-if=\"showImgTitle || showImgText\" mbr-class=\"{'mbr-px-2': !transparentBg &amp;&amp; countImg == 1, 'mbr-col-sm-12':(!transparentBg &amp;&amp; countImg != 1) || (transparentBg &amp;&amp; countImg == 1)}\">\n       