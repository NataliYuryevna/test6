          "_name": "features4",
          "_customHTML": "<section class=\"features4\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Line\" name=\"showLine\" checked=\"\">\n        <input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#bbc3cc\" condition=\"showLine\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show List\" name=\"showList\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"String Count\" name=\"listGrow\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"showList\">\n        <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\" condition=\"showList\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Size Icons\" name=\"sizeIcons\" min=\"0.1\" max=\"2\" step=\"0.2\" value=\"0.9\" condition=\"showList &amp;&amp; showIcons\">\n        <input type=\"color\" title=\"Icons Color\" name=\"iconsColor\" value=\"#767676\" condition=\"showList &amp;&amp; showIcons\">\n\n        <input type=\"checkbox\" title=\"Media on Left/Right\" name=\"flexDirection\">\n\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background3.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#f4f5f6\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type!='color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n        <input type=\"range\" inline=\"\" title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"container-fluid\">\n        <div class=\"mbr-row main-row\" mbr-class=\"{'mbr-row-reverse':flexDirection == true}\">\n            <div class=\"image-block mbr-col-lg-6 mbr-col-md-6 mbr-col-sm-12\">\n                <amp-img class=\"mobirise-loader\" src=\"@PROJECT_PATH@/assets/images/photo2-1200x800.jpg\" layout=\"responsive\" width=\"741\" height=\"494\" alt=\"Mobirise\">\n                    <div placeholder=\"\" class=\"placeholder\">\n                        <div class=\"mobirise-spinner\">\n                            <em></em>\n                            <em></em>\n                            <em></em>\n                        </div>\n                    </div>\n                    <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/photo2-1200x800.jpg\" alt=\"Mobirise\" title=\"\">\n                </amp-img>\n            </div>\n            <div class=\"text-block d-flex mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-6\">\n                <div class=\"text-wrapper mbr-col-lg-9\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style mbr-black mbr-bold align-left mbr-mb-20\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .line\">3 Reasons To Choose Us</h2>\n                    <div class=\"line align-left mbr-mb-30\">\n                        <div mbr-if=\"showLine \" class=\"line-bottom\"></div>\n                    </div>\n                    <h3 class=\"mbr-section-subtitle mbr-fonts-style align-left mbr-mb-30 l-h-15\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-section-subtitle\">To achieve the best result and we use the three principles in our work</h3>\n                    <div class=\"mbr-text counter-container mbr-fonts-style mbr-mt-30\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text\" mbr-if=\"showList\" data-multiline=\"\">\n                        <ul class=\"list-padding\" mbr-list-grow=\"listGrow\" mbr-list=\"\">\n                            <li class=\"list-item mbr-mb-8 d-flex mbr-align-items-center\">\n                                <div class=\"card-img d-flex mbr-justify-content-center mbr-align-items-center\" mbr-if=\"showIcons\">\n                                    <div class=\"icon-wrap\">\n                                        <span mbr-icon=\"\" class=\"mbr-iconfont fa-check fa\"></span>\n                                    </div>\n                                </div>\n                                <p class=\"list-item-content\">Experience </p>\n                            </li>\n                            <li class=\"list-item mbr-mb-8 d-flex mbr-align-items-center\">\n                                <div class=\"card-img d-flex mbr-justify-content-center mbr-align-items-center\" mbr-if=\"showIcons\">\n                                    <div class=\"icon-wrap\">\n                                        <span mbr-icon=\"\" class=\"mbr-iconfont fa-check fa\"></span>\n                                    </div>\n                                </div>\n                                <p class=\"list-item-content\">Professionality</p>\n                            </li>\n                            <li class=\"list-item mbr-mb-8 d-flex mbr-align-items-center\">\n                                <div class=\"card-img d-flex mbr-justify-content-center mbr-align-items-center\" mbr-if=\"showIcons\">\n                                    <div class=\"icon-wrap\">\n                                        <span mbr-icon=\"\" class=\"mbr-iconfont fa-check fa\"></span>\n                                    </div>\n                                </div>\n                                <p class=\"list-item-content\">Guarantee</p>\n                            </li>\n                        </ul>\n                    </