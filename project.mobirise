        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card, .card-img": {
              "width": "100%"
            },
            ".card-wrapper when not (@transparentBg), (@showBorders)": {
              "@media (min-width: 992px)": {
                "padding": "40px 30px"
              }
            },
            ".card-wrapper when not (@transparentBg)": {
              "background-color": "@cardBg"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-img amp-img": {
              "width": "~\"@{imageWidth}%\"",
              "max-width": "370px",
              "min-width": "60px"
            },
            ".card-subtitle:before when (@lineSpecialty) and (@showSpecialty)": {
              "content": "''",
              "width": "25px",
              "height": "3px",
              "display": "inline-block",
              "vertical-align": "middle",
              "margin-right": "10px",
              "background-color": "@primaryColor"
            },
            ".icons-list .amp-iconfont when (@showSocial)": {
              "font-size": "~\"calc(@{iconsSize}rem * 0.42)\"",
              "line-height": "~\"calc(@{iconsSize}rem - 4px)\"",
              "width": "~\"@{iconsSize}rem\"",
              "height": "~\"@{iconsSize}rem\"",
              "margin-bottom": "10px",
              "& when ((@iconsSize * 16) > 30)": {
                "@media (max-width: 991px)": {
                  "font-size": "~\"calc(30px * 0.42)\"",
                  "line-height": "26px",
                  "width": "30px",
                  "height": "30px"
                }
              }
            },
            ".mbr-row when not (@showPadding)": {
              ".card": {
                "padding": "0"
              },
              "& when (@showBorders)": {
                "& when (@showTopBorder)": {
                  "border-top": "1px solid @bordersColor"
                },
                ".card-wrapper": {
                  "border": "none"
                },
                "@media (min-width: 768px)": {
                  ".card:not(.last-child) .card-wrapper": {
                    "border-right": "1px solid @bordersColor"
                  }
                },
                "@media (max-width: 991px)": {
                  ".card.md-pb .card-wrapper when (@cardsAmount = 4)": {
                    "border-bottom": "1px solid @bordersColor"
                  },
                  ".card.md-pb:nth-child(2) .card-wrapper when (@cardsAmount = 4)": {
                    "border-right": "none"
                  }
                },
                "@media (max-width: 767px)": {
                  ".card:not(.last-child) .card-wrapper": {
                    "border-right": "none",
                    "border-bottom": "1px solid @bordersColor"
                  }
                }
              }
            }
          },
