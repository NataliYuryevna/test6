        {
          "_styles": {
            "padding": "0!important",
            ".mbr-overlay": {
              "background": "@overlayColor",
              "opacity": "@overlayOpacity",
              "pointer-events": "none",
              "z-index": "1"
            },
            ".slider-box": {
              "height": "100%"
            },
            ".carousel-img": {
              "align-self": "flex-start",
              "position": "relative",
              "align-items": "center",
              ".card-box": {
                "position": "absolute",
                "z-index": "2",
                "top": "50%",
                "transform": "translateY(-50%)"
              }
            },
            ".carousel, .carousel-img, .carousel-img amp-img, .carousel-img amp-img img": {
              "height": "~\"@{heightImg}rem\"",
              "width": "100%"
            },
            "& when (@fullScreen)": {
              "align-items": "stretch",
              ".carousel, .carousel-img amp-img, .carousel-img amp-img img": {
                "height": "100%"
              },
              ".carousel-img, &": {
                "height": "100vh"
              }
            },
            "img": {
              "object-fit": "cover"
            },
            ".amp-carousel-button": {
              "width": "~\"calc(@{buttonSize}px * 5)\"",
              "height": "~\"calc(@{buttonSize}px * 5)\"",
              "background-color": "@buttonColor",
              "&:after": {
                "border-color": "contrast(@buttonColor, darken(@buttonColor, 50%), #ffffff, 60%) !important",
                "width": "~\"@{buttonSize}px\"",
                "height": "~\"@{buttonSize}px\""
              },
              ".amp-carousel-button-next:after": {
                "left": "~\"calc(50% - @{buttonSize}px/4)\""
              },
              ".amp-carousel-button-prev:after": {
                "left": "~\"calc(50% + @{buttonSize}px/4)\""
              }
            },
            "amp-carousel when (@effectButton)": {
              ".amp-carousel-button": {
                "opacity": "0",
                "&:after, &": {
                  "-webkit-transition": "all 0.3s",
                  "transition": "all 0.3s"
                }
              },
              "&:hover .amp-carousel-button": {
                "opacity": "1"
              },
              ".amp-carousel-button:hover": {
                "opacity": "0.75",
                "&:after": {
                  "width": "~\"calc(@{buttonSize}px * 1.25)\"",
                  "height": "~\"calc(@{buttonSize}px * 1.25)\""
                }
              }
            },
            "@media (max-width: 767px)": {
              ".amp-carousel-button": {
                "display": "none"
              }
            },
            ".card-box when (@showPadding)": {
              "@media (min-width: 768px)": {
                "padding-left": "30px",
                "padding-right": "30px"
              },
              "@media (min-width: 992px)": {
                "padding-left": "60px",
                "padding-right": "60px"
              },
              "@media (min-width: 1200px)": {
                "padding-left": "105px",
                "padding-right": "105px"
              }
            },
            ".mbr-section-btn when (@showSubtitle), (@showTitle), (@showText)": {
              "padding-top": "45px"
            }
          },
          "_name": "header6",
          "_customHTML": "<section class=\"header6 slider\" group=\"Sliders\" plugins=\"amp-main, amp-carousel\" mbr-class=\"{'mbr-fullscreen': fullScreen}\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n    <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n    <input type=\"range\" inline=\"\" title=\"Height Image\" name=\"heightImg\" min=\"10\" max=\"50\" step=\"1\" value=\"30\" condition=\"!fullScreen\">        \n    <header>Show/Hide</header>\n    <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n    <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked=\"\">\n    <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked=\"\">\n    <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked=\"\">\n    <select title=\"Align\" name=\"alignTitle\" condition=\"showTitle || showSubtitle || showText || showButtons\">\n        <option value=\"1\" selected=\"\">Left</option>\n        <option value=\"2\">Center</option>\n        <option value=\"3\">Right</option>\n    </select>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\">\n        <input type=\"range\" inline=\"\" title=\"Delay\" name=\"delay\" min=\"1000\" max=\"5000\" step=\"1000\" value=\"3000\" condition=\"autoplay\">\n        <select title=\"Images\" name=\"imagesAmount\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\" selected=\"\">6</option>\n        </select>\n    <input type=\"checkbox\" title=\"Padding Text\" name=\"showPadding\" checked=\"\">   \n    <input type=\"checkbox\" title=\"Effect Carousel Button\" name=\"effectButton\" checked=\"\">\n    <input type=\"color\" title=\"Carousel Button Color\" name=\"buttonColor\" value=\"#ffffff\">\n    <input type=\"range\" inline=\"\