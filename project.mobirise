          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@fullHeight) and not (@showTitle) and not (@showSubtitle)": {
              "padding": "0",
              ".card-img, .card-img img, .card-img amp-img, .card-box": {
                "min-height": "450px",
                "@media (max-width: 991px)": {
                  "min-height": "350px"
                }
              },
              ".card-box when not (@showImg)": {
                "@media (min-width: 992px)": {
                  "min-height": "900px"
                }
              },
              ".card-box when not (@showImg) and (@transparentBg) and not (@showBorders)": {
                "min-height": "270px"
              }
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-benefits-box when (@showCardsTitleNumber)": {
              "align-items": "center",
              ".card-number when (@showLineNumber) and (@showCardsNumber)": {
                "position": "relative",
                "padding-left": "70px",
                "& when not (@transparentBg), (@showBorders)": {
                  "& when not (@fullWidth)": {
                    "padding-left": "30px"
                  }
                },
                "&:before": {
                  "content": "''",
                  "width": "50px",
                  "& when not (@transparentBg), (@showBorders)": {
                    "& when not (@fullWidth)": {
                      "width": "10px"
                    }
                  },
                  "height": "1px",
                  "position": "absolute",
                  "left": "10px",
                  "top": "50%",
                  "transform": "translateY(-50%)",
                  "background-color": "@lineColor"
                }
              }
            },
            ".card-box when (@showCardsTitleNumber), (@showCardsTitle), (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
              "width": "100%",
              "& when (@showBorders), not (@transparentBg)": {
                "padding": "60px",
                "@media (max-width: 991px)": {
                  "padding": "25px"
                }
              }
            },
            ".card-wrapper, .card-wrapper .card-box": {
              "position": "relative"
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-wrapper when (@showCardsEffect)": {
              "& when (@showCardsTitleNumber), (@showCardsTitle)": {
