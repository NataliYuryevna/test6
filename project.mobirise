          "_name": "features11",
          "_customHTML": "<section class=\"features11\" group=\"Features\" plugins=\"amp-main\" mbr-class=\"{'mbr-fullscreen':fullScreen}\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"5\" condition=\"!fullScreen\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"5\" condition=\"!fullScreen\">\n        \n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Line\" name=\"showLine\" checked=\"\">\n        <input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#bbc3cc\" condition=\"showLine\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\" checked=\"\">\n        <select title=\"Comment Count\" name=\"commentCount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected=\"\">2</option>\n        </select>\n        <input type=\"color\" title=\"Comment BG Color\" name=\"commentBgColor\" value=\"#ffffff\">\n        <input type=\"checkbox\" title=\"Show Photo\" name=\"showPhoto\" checked=\"\">\n        <input type=\"checkbox\" title=\"Media on Left/Right\" name=\"flexDirection\">\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background7.jpg\" selected=\"\">\n            <input type=\"color\" title=\"Background Color\" value=\"#f2f0ed\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </div>\n    <div class=\"container\">\n        <div class=\"mbr-row main-row\" mbr-class=\"{'mbr-row-reverse':flexDirection == true}\">\n            <div class=\"block-content mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-4\">\n                <div class=\"content-wrapper\">\n                    <h2 class=\"mbr-section-title align-left mbr-bold mbr-fonts-style mbr-mb-8 mbr-white\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .line\">\n                        Our students\n                    </h2>\n                    <div class=\"line align-left\">\n                        <div mbr-if=\"showLine \" class=\"line-bottom\"></div>\n                    </div>\n                    <p class=\"mbr-text text mbr-fonts-style mbr-mt-30 grey-color\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".text, .mbr-section-btn\"><br>Do you want to go to our school?&nbsp;Take a test to determine the level of knowledge of language skills</p>\n                    <div mbr-buttons=\"\" class=\"mbr-section-btn mbr-mt-50 width-full\" mbr-theme-style=\"display-7\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\">\n                        <a class=\"btn btn-black btn-md mbr-light\" href=\"https://mobirise.com\">Details</a>\n                    </div>\n                </div>\n            </div>\n            <div class=\"block-content mbr-flex-column mbr-justify-content-center mbr-align-items-center mbr-col-sm-12\" mbr-class=\"{'mbr-col-lg-8 mbr-col-md-6':commentCount==1,\n                        'mbr-col-lg-4 mbr-col-md-6':commentCount==2}\">\n                <div class=\"card-wrapper width-full mbr-p-60\">\n                    <p class=\"card-text mbr-fonts-style main-text align-left mbr-black\" mbr-theme-style=\"display-7\" data-app-selector=\".main-text\">A wonderful school and wonderful teachers!</p>\n                    <div class=\"id-wrapper\">\n                        \n                        <p class=\"item-text grey-color mbr-text align-left mbr-fonts-style mbr-mt-16\" mbr-theme-style=\"display-7\" data-app-selector=\".item-text\">\n                            Cook\n                        </p>\n                    </div>\n                </div>\n                <div class=\"triangle-bottom\"></div>\n                <div class=\"card-img d-flex mbr-justify-content-center mbr-mt-8\" mbr-if=\"showPhoto\">\n                    <amp-img class=\"mobirise-loader\" src=\"@PROJECT_PATH@/assets/images/face5.jpg\" layout=\"responsive\" width=\"50\" height=\"50\" alt=\"Mobirise\" mbr-if=\"showPhoto\">\n                        <div placeholder=\"\" class=\"placeholder\">\n                            <div class=\"mobirise-spinner\">\n                                <em></em>\n                                <em></em>\n                                <em></em>\n                            </div>\n                        </div>\n                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/face5.jpg\" alt=\"Mobirise\">\n                    </amp-img>\n                </div>\n            </div>\n            <div class=\"block-content last-item mbr-flex-column mbr-justify-content-center mbr-align-items-center mbr-col-lg-4 mbr-col-md-12 mbr-col-sm-12\" mbr-if=\"commentCount>1\">\n                <div class=\"card-wrapper width-full mbr-p-60\">\n                    <p class=\"card-text mbr-fonts-style main-text align-left mbr-black\" mbr-theme-style=\"display-7\" data-app-selector=\".main-text\">Nice school!</p>\n                    <div class=\"id-wrapper\">\n                   