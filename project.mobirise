        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".elements-block, .slider-box, .slide, .dots-wrapper, .second-element-box, .text-content": {
              "width": "100%"
            },
            ".container-img, .text-wrap, .text-box": {
              "height": "100%"
            },
            ".elements-block, .second-element, .image-wrap, .slider-box, amp-carousel": {
              "position": "relative"
            },
            ".text-wrap when (@showImgTitle), (@showImgText)": {
              "& when not (@testimonialsBg)": {
                "background-color": "@textBg",
                "padding": "0"
              },
              ".mbr-content-subtitle:before when (@lineTitle) and (@showImgTitle)": {
                "content": "''",
                "width": "25px",
                "height": "3px",
                "margin-right": "10px",
                "vertical-align": "middle",
                "background-color": "@primaryColor"
              }
            },
            "& when (@showImgText), (@showImgTitle), (@showImg)": {
              ".slider-box, amp-carousel": {
                "height": "(@slideHeight * 1px)"
              },
              "amp-carousel .slide": {
                "align-self": "flex-start"
              },
              ".slide": {
                "height": "~\"calc(@{slideHeight}px - 35px)\"",
                "margin-bottom": "35px",
                "amp-img,img": {
                  "object-fit": "cover"
                },
                ".container-img img when (@showImg)": {
                  "-webkit-transition": "transform 5s linear",
                  "transition": "transform 5s linear"
                },
                "&:hover .container-img img when (@showImg)": {
                  "-webkit-transform": "scale(1.15)",
                  "transform": "scale(1.15)"
                }
              },
              ".dots span, .amp-carousel-button": {
                "background-color": "@buttonsColor"
              },
              ".dots span": {
                "border-radius": "10px",
                "width": "9px",
                "height": "9px",
                "margin": "0 3px",
                "opacity": "0.4",
                "outline": "none"
              },
              ".dots span.current": {
                "opacity": "1",
                "width": "18px"
              },
              ".amp-carousel-button": {
                "display": "none"
              }
            },
            "& when (@testimonialsPadding) and (@showImg)": {
              ".text-box when (@showImgTitle), (@showImgText)": {
                "@media (min-width: 992px)": {
                  "& when not (@testimonialsReverseContent)": {
                    "padding-left": "50px"
                  },
                  "& when (@testimonialsReverseContent)": {
                    "padding-right": "50px"
                  }
                }
              }
            },
            ".second-element when (@showPadding)": {
              "& when (@showImgText), (@showImgTitle), (@showImg)": {
                "@media (min-width: 992px)": {
                  "& when not (@reverseContent)": {
                    "padding-right": "70px"
                  },
                  "& when (@reverseContent)": {
                    "padding-left": "70px"
                  }
                },
                "& when (@fullWidth)": {
                  "@media (min-width: 1200px)": {
                    "padding-left": "100px",
                    "padding-right": "100px"
                  }
                }
              }
            },
            ".container-img when (@showImg)": {
              "padding": "0"
            },
            ".elements-block .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".milestone:before when (@showBorderTop) and (@showMilestone)": {
              "content": "\"\"",
              "width": "100%",
              "height": "1px",
              "background-color": "@borderColor",
              "margin": "50px 0"
            },
            ".marker-wrap:before, .dots span, .mbr-content-subtitle:before": {
              "display": "inline-block"
            },
            ".mbr-list when (@stylizedCounters = \"1\") and (@showCardList)": {
              "counter-reset": "decimal",
              ".marker-wrap:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "& when (@countersBorder)": {
                  "border-bottom": "2px solid @primaryColor"
                }
              }
            },
            ".marker-wrap:before when (@stylizedCounters = \"2\") and (@showCardList)": {
              "content": "\"\"",
              "border": "3px solid @primaryColor",
              "width": "~\"calc(@{widthImg}rem - 6px)\"",
              "height": "~\"calc(@{widthImg}rem - 6px)\"",
              "box-sizing": "initial"
            },
            ".mbr-list .items-list when (@showCardList)": {
              "flex-direction": "row",
              "& when not (@stylizedCounters = \"3\")": {
                "align-items": "baseline"
              },
              ".marker-wrap, .icon-wrap": {
                "margin-right": "15px"
              },
              "&:not(:last-child)": {
                "padding-bottom": "10px"
              },
              "& when ((@widthImg * 16) > 30)": {
                ".marker-wrap:before, .icon-wrap .amp-iconfont": {
                  "@media (max-width: 767px)": {
                    "font-size": "30px",
                    "width": "30px",
                    "height": "30px"
                  }
                }
              }
            },
            ".icon-wrap .amp-iconfont when (@stylizedCounters = \"3\") and (@showCardList)": {
              "color": "@primaryColor",
              "font-size": "~\"@{widthImg}rem\"",
              "width": "~\"@{widthImg}rem\""
            },
            "@media (min-width: 768px)": {
              ".milestone when (@showMilestone)": {
                "> div:not(:first-child) when (@showBorderRight) and not (@columnWidth = 5)": {
                  "border-left": "1px solid @borderColor"
                },
                "> div:not(:last-child) when (@columnWidth = 5)": {
                  "margin-bottom": "20px"
                },
                "& when (@milCount > 2) and not (@columnWidth = 5)": {
                  "& > div:nth-child(2n+1)": {
                    "border-left": "none"
                  },
                  "& > div:not(:last-child)": {
                    "margin-bottom": "20px"
                  },
                  "& > div:nth-last-child(2) when (@milCount = 4)": {
                    "margin-bottom": "0px"
                  }
                }
              },
              ".second-element-box .milestone > div when (@milCount = 3) and (@fullWidth) and not (@columnWidth = 5) and (@showMilestone)": {
                "@media (min-width: 1200px)": {
                  "margin-bottom": "0px",
                  "&:not(:first-child) when (@showBorderRight)": {
                    "border-left": "1px solid @borderColor"
                  }
                }
              },
              "& when not (@showBullets)": {
                "& when (@showImgText), (@showImgTitle), (@showImg)": {
                  ".slide": {
                    "height": "~\"calc(@{slideHeight}px - 85px)\"",
                    "margin-bottom": "85px"
                  },
                  ".amp-carousel-button": {
                    "display": "block",
                    "top": "~\"calc(100% - 27px)\"",
                    "opacity": "0.8",
                    "&:after": {
                      "border-color": "contrast(@buttonsColor, darken(@buttonsColor, 50%), #ffffff, 60%) !important"
                    },
                    "&:hover": {
                      "opacity": "1"
                    }
                  },
                  ".amp-carousel-button-prev": {
                    "left": "0px"
                  },
                  ".amp-carousel-button-next": {
                    "left": "55px"
                  },
                  ".dots-wrapper": {
                    "display": "none"
                  }
                }
              }
            },
            "@media (max-width: 767px)": {
              ".container-img when (@showImgText), (@showImgTitle)": {
                "height": "50%"
              },
              ".text-wrap when (@showImg)": {
                "height": "50%"
              },
              ".second-element when (@showImgText), (@showImgTitle), (@showImg)": {
                "padding-bottom": "50px"
              },
              ".text-wrap when (@showImg) and (@testimonialsBg)": {
                "padding-top": "25px"
              },
              ".milestone when (@showMilestone)": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                }
              }
            }
