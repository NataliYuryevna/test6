                  "@media (max-width: 767px)": {
                    "height": "30px",
                    "width": "30px"
                  }
                }
              },
              ".icon-wrap .amp-iconfont": {
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg1Icons}rem\"",
                "width": "~\"@{widthImg1Icons}rem\"",
                "& when ((@widthImg1Icons * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "font-size": "30px",
                    "width": "30px"
                  }
                }
              },
              ".marker-wrap, .icon-wrap, .counters-wrap": {
                "margin-right": "15px"
              },
              ".items-list:not(:last-child)": {
                "padding-bottom": "10px"
              }
            },
            ".items-list": {
              "flex-direction": "row"
            },
            "@media (max-width: 767px)": {
              ".second-element when (@transparentBg)": {
                "padding-top": "50px"
              },
              ".milestone when (@showMilestone)": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                },
                "& > div:not(:first-child)": {
                  "margin-top": "20px"
                }
              }
            },
            ".list2 .line-wrap:before, .list1 .marker-wrap:before": {
              "margin-top": "9px"
            },
            ".list2 when (@showCardList2)": {
              ".line-wrap:before": {
                "content": "\"\"",
                "background-color": "@primaryColor",
                "margin-right": "5px",
                "width": "~\"@{widthImg2}rem\"",
                "height": "3px",
                "& when ((@widthImg2 * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "width": "30px"
                  }
                }
              },
              ".marker-wrap:before": {
                "content": "\"✓\"",
                "font-size": "~\"@{widthImg2}rem\"",
                "margin": "6px 13px 0 0 ",
                "& when ((@widthImg2 * 16) > 30)": {
                  "@media (max-width: 767px)": {
                    "font-size": "30px"
                  }
                }
              }
            },
            "@media (min-width: 768px)": {
              ".ampTabContainer": {
                "display": "flex",
                "flex-wrap": "wrap",
                "padding": "0"
              },
              ".milestone > .mbr-col-md-6 when (@showMilestone)": {
                "&:nth-child(2) when (@columnWidth = 5)": {
                  "margin-top": "20px"
                },
                "&:nth-child(3), &:nth-child(4)": {
                  "margin-top": "20px"
                },
                "&.mbr-col-xl-4:nth-child(3)": {
                  "@media (min-width: 1200px)": {
                    "margin-top": "0"
                  }
                },
                "& when (@showBorderRight)": {
                  "&:nth-child(2n)": {
                    "border-left": "1px solid @borderColor"
                  },
                  "&.mbr-col-xl-4:nth-child(3)": {
                    "@media (min-width: 1200px)": {
                      "border-left": "1px solid @borderColor"
                    }
                  }
                }
              }
            },
            ".tabContent": {
              "display": "none",
              "order": "1",
              "> div": {
                "padding": "55px 30px 30px 30px",
                "@media (max-width: 991px)": {
                  "padding": "55px 15px 30px 15px"
                },
                "order": "1",
                "background-color": "@SliderBg"
              }
            },
            ".tabButton": {
              "padding": "15px 30px",
              "@media (max-width: 991px)": {
                "padding": "10px 15px"
              },
              "@media (max-width: 767px)": {
